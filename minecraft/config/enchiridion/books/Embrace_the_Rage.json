          "yPos": 14,
          "width": 48.0,
          "height": 12.0,
          "feature": {
            "class": "joshie.enchiridion.gui.book.features.FeatureText",
            "properties": {
              "text": "THE MODS",
              "size": 1.0
            }
          },
          "isLocked": true,
